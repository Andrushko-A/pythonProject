from collections import Counter
text = input()
arr = dict(Counter(text.lower()).most_common())
count_symb = dict(arr)
print(count_symb) 
descrybing = ''
encrybing = ''

alphabet = 'абвгдеёжзийклмнопрстуфхцчшщъыьэюя. '
step = 3
for i in text:
  place = alphabet.find(i)
  new_place = (place + step)%33
  if i in alphabet:
    descrybing += alphabet[new_place]
  else:
    descrybing += i
print(descrybing)

descr = dict(Counter(descrybing).most_common())
print(descr)

slovar = dict(zip(arr.keys(), descr.keys()))
print(slovar)

for i in descrybing:
  plac = slovar.values.find(i)
  if i in slovar.values:
    encrybing += slovar.keys(i)
  else:
    encrybing += i
print(encrybing)


